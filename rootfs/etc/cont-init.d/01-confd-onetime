#!/usr/bin/execlineb -P
with-contenv
multisubstitute
{
  import -u -D"/stash" CONFD_PREFIX
  import -u -D"env" CONFD_BACKEND
}
confd --onetime --prefix="${CONFD_PREFIX}" --tmpl-src="/etc/stash/stash-config.properties.tmpl" --tmpl-dest="/var/lib/stash/shared/stash-config.properties" ${CONFD_BACKEND}
